{
  "name": "arquebus",
  "description": "Tiny framework for redis-persisted background jobs",
  "author": "Cody Greene <cody@dudewoah.com>",
  "keywords": ["redis", "worker", "delayed", "queue"],
  "version": "1.0.0",
  "license": "MIT",
  "repository": "cody-greene/arquebus",
  "engines": {"node": "^4.0.0"},
  "files": ["lib/"],
  "main": "lib/index.js",
  "eslintConfig": {
    "extends": "./node_modules/eslint-config/eslintrc.yml",
    "env": {
      "browser": false,
      "es6": true
    }
  },
  "scripts": {
    "hydra": "node --harmony example/hydra",
    "peon": "node --harmony example/peon",
    "foreman": "node --harmony example/foreman",
    "enqueue": "node --harmony example/enqueue",
    "test": "mocha --harmony --bail"
  },
  "devDependencies": {
    "eslint-config": "cody-greene/eslint-config#v0.1.1",
    "mocha": "^2.2.5",
    "redis": "^2.4.2"
  }
}
